{
  "name": "squirrel-shop",
  "version": "0.3.2",
  "private": true,
  "scripts": {
    "dev": "dotenv -e .env.local -- next dev --turbopack",
    "build": "dotenv -e .env.production -- next build",
    "build:analyze": "cross-env ANALYZE=true dotenv -e .env.production -- next build",
    "start": "dotenv -e .env.local -- next start",
    "start:prod": "dotenv -e .env.production -- next start",
    "lint": "next lint",
    "lint:tests": "eslint \"src/**/*.test.{ts,tsx}\" \"src/**/__tests__/**/*.{ts,tsx}\"",
    "lint:file": "eslint",
    "prisma:push:local": "dotenv -e .env.local -- prisma db push",
    "prisma:generate:local": "dotenv -e .env.local -- prisma generate",
    "prisma:seed:local": "dotenv -e .env.local -- prisma db seed",
    "prisma:studio:local": "dotenv -e .env.local -- prisma studio",
    "prisma:push:prod": "dotenv -e .env.production -- prisma db push",
    "prisma:generate:prod": "dotenv -e .env.production -- prisma generate",
    "prisma:seed:prod": "dotenv -e .env.production -- prisma db seed",
    "prisma:studio:prod": "dotenv -e .env.production -- prisma studio",
    "test": "jest -o",
    "test:global": "jest",
    "test:silent": "jest --silent",
    "test:watch": "jest --watch",
    "test:webkit": "tsx ./src/utils/playwright/webkit.ts"
  },
  "prisma": {
    "seed": "tsx prisma/seed.ts",
    "studio": "tsx prisma/studio.ts",
    "generate": "tsx prisma/generate.ts",
    "push": "tsx prisma/push.ts"
  },
  "dependencies": {
    "@auth/prisma-adapter": "^2.9.1",
    "@hookform/resolvers": "^5.0.1",
    "@prisma/client": "^6.8.2",
    "@stripe/stripe-js": "^7.3.1",
    "@tanstack/react-query": "^5.81.5",
    "@uploadthing/react": "^7.3.1",
    "@vercel/analytics": "^1.5.0",
    "@vercel/speed-insights": "^1.2.0",
    "bcryptjs": "^3.0.2",
    "clsx": "^2.1.1",
    "embla-carousel": "^8.6.0",
    "embla-carousel-autoplay": "^8.6.0",
    "embla-carousel-react": "^8.6.0",
    "linkify-it": "^5.0.0",
    "lucide-react": "^0.511.0",
    "motion": "^12.23.2",
    "next": "15.3.2",
    "next-auth": "^5.0.0-beta.28",
    "next-themes": "^0.4.6",
    "nodemailer": "^7.0.5",
    "qs": "^6.14.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-hook-form": "^7.57.0",
    "react-hot-toast": "^2.5.2",
    "stripe": "^18.2.1",
    "uploadthing": "^7.7.2",
    "uuid": "^11.1.0",
    "zod": "^3.25.51",
    "zustand": "^5.0.5"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@next/bundle-analyzer": "^15.3.5",
    "@tailwindcss/postcss": "^4",
    "@tanstack/eslint-plugin-query": "^5.81.2",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.6.4",
    "@testing-library/react": "^16.3.0",
    "@testing-library/react-hooks": "^8.0.1",
    "@testing-library/user-event": "^14.6.1",
    "@types/jest": "^30.0.0",
    "@types/linkify-it": "^5.0.0",
    "@types/node": "^20",
    "@types/nodemailer": "^6.4.17",
    "@types/qs": "^6.14.0",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "cross-env": "^7.0.3",
    "dotenv-cli": "^8.0.0",
    "eslint": "^9",
    "eslint-config-next": "15.3.2",
    "eslint-plugin-jest": "^29.0.1",
    "eslint-plugin-jest-dom": "^5.5.0",
    "eslint-plugin-testing-library": "^7.6.6",
    "jest": "^30.0.5",
    "jest-environment-jsdom": "^30.0.5",
    "playwright": "^1.56.1",
    "prettier": "^3.5.3",
    "prettier-plugin-tailwindcss": "^0.6.11",
    "prisma": "^6.8.2",
    "tailwindcss": "^4",
    "tsx": "^4.19.4",
    "typescript": "^5"
  }
}
